---
import Container from "../components/Container.astro";
import Profile from "../components/Profile.astro";
import Heading from "../components/Heading.astro";
import Section from "../components/Section.astro";
import { getEntry } from "astro:content";

const organisers = await Promise.all([
  getEntry("member", "pawel-grzybek"),
  getEntry("member", "darrn-sharp"),
]);

interface Contributor {
  role: string;
  totalAmountDonated: number;
  name: string;
  image?: string;
}

const fetchContributors = async () => {
  try {
    const response = await fetch(
      "https://opencollective.com/nn1-dev/members/all.json",
    );

    if (!response.ok) {
      return [] as Array<Contributor>;
    }
    const responseJson: Array<Contributor> = await response.json();

    return responseJson;
  } catch (e) {
    console.error("Error fetching contributors", e);
    return [] as Array<Contributor>;
  }
};

const contributors = await fetchContributors();
const contributorsData = contributors
  .filter(({ role }) => role === "BACKER")
  .sort((a, b) => b.totalAmountDonated - a.totalAmountDonated);

interface Props {
  founders?: boolean;
  partners?: boolean;
  supporters?: boolean;
}

const { founders = true, partners = true, supporters = true } = Astro.props;
