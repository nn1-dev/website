<style>
  .organisers {
    display: flex;
    flex-direction: column;
    gap: calc(var(--lh) * 3);
  }

  .organisers__section {
    display: flex;
    flex-direction: column;
    gap: var(--lh);
  }

  .organisers__grid {
    display: grid;
    gap: calc(var(--lh) * 2);

    @media (800px <= width) {
      grid-template-columns: repeat(2, 1fr);
    }
  }

  .organisers__partners {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: calc(var(--lh) / 2);

    @media (600px <= width) {
      grid-template-columns: repeat(3, 1fr);
    }

    @media (1000px <= width) {
      grid-template-columns: repeat(6, 1fr);
    }
  }

  .organisers__partner {
    display: flex;
    justify-content: center;
